+++ b/jules-scratch/verification/verify_my_bookings.py
@@ -0,0 +1,50 @@
+from playwright.sync_api import sync_playwright, Page, expect
+
+def verify_my_bookings_page(page: Page):
+    """
+    This script verifies that the 'My Bookings' page for sitters renders correctly,
+    with the 'Upcoming' tab selected by default.
+    """
+    # 1. Navigate to the page
+    page.goto("http://localhost:3000/sitter/my-bookings")
+
+    # 2. Verify the heading is visible
+    expect(page.get_by_role("heading", name="My Bookings")).to_be_visible()
+
+    # 3. Verify the "Upcoming" tab is active by default
+    upcoming_tab = page.get_by_role("button", name="Upcoming")
+    expect(upcoming_tab).to_have_class(
+        "border-blue-500 text-blue-600 whitespace-nowrap py-4 px-1 border-b-2 font-medium text-sm"
+    )
+
+    # 4. Verify the content for the "Upcoming" tab is displayed
+    expect(page.get_by_role("heading", name="Upcoming Bookings")).to_be_visible()
+
+    # 5. Take a screenshot for visual confirmation
+    page.screenshot(path="jules-scratch/verification/my-bookings-page.png")
+
+    # 6. Click the "Past" tab
+    past_tab = page.get_by_role("button", name="Past")
+    past_tab.click()
+
+    # 7. Verify the "Past" tab is now active
+    expect(past_tab).to_have_class(
+      "border-blue-500 text-blue-600 whitespace-nowrap py-4 px-1 border-b-2 font-medium text-sm"
+    )
+    expect(page.get_by_role("heading", name="Past Bookings")).to_be_visible()
+
+    # 8. Take a final screenshot showing the "Past" tab selected
+    page.screenshot(path="jules-scratch/verification/my-bookings-past-tab.png")
+
+
+if __name__ == "__main__":
+    with sync_playwright() as p:
+        browser = p.chromium.launch(headless=True)
+        page = browser.new_page()
+        try:
+            verify_my_bookings_page(page)
+            print("Verification script ran successfully.")
+        except Exception as e:
+            print(f"An error occurred: {e}")
+        finally:
+            browser.close()
\ No newline at end of file
